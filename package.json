{
  "name": "pwa",
  "version": "1.0.0",
  "description": "A PWA to display Discogs vinyl information ",
  "private": true,
  "scripts": {
    "build:prod": "mkdir dist && npm run build:app:prod && npm run netlify:build",
    "build:server:dev": "webpack --mode development --config webpack.server.config.js",
    "build:server:prod": "webpack --mode production --config webpack.server.config.js",
    "build:app:dev": "webpack --mode development --config webpack.dev.config.js --profile --json > ./dist/stats.json",
    "build:app:prod": "webpack --mode production --config webpack.prod.config.js --profile --json > ./dist/stats.json",
    "start:dev": "NODE_ENV=development nodemon ./dist/server.js --verbose",
    "start:prod": "NODE_ENV=production node ./dist/server.js",
    "netlify:build": "netlify-lambda build ./src/lambda",
    "netlify:serve": "netlify-lambda serve ./src/lambda",
    "lint": "eslint src -c .eslintrc.json --ext js",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybeeching/pwa.git"
  },
  "author": "Andrew Beeching",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybeeching/pwa/issues"
  },
  "homepage": "https://github.com/andybeeching/pwa#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@babel/runtime-corejs3": "^7.5.5",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "helmet": "^3.21.0",
    "lru-cache": "^5.1.1",
    "node-fetch": "^2.6.0",
    "normalize.css": "^8.0.1",
    "remove-blank-lines": "^1.1.1",
    "serve-static": "^1.14.1",
    "serverless-http": "^2.3.0",
    "spdy": "^4.0.1",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-raw-loader": "^1.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "netlify-lambda": "^1.6.3",
    "nock": "^11.3.3",
    "nodemon": "^1.19.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "1.18.2",
    "prettier-config-standard": "^1.0.1",
    "raw-loader": "^3.1.0",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-node-externals": "^1.7.2"
  }
}
